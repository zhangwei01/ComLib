<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- disabled -->
    <item android:drawable="@drawable/ic_switch_trans" android:state_enabled="false"/>
    <!-- pressed -->
    <item android:drawable="@drawable/ic_switch_trans" android:state_pressed="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/ic_switch_focused" android:state_focused="true"/>
    <!-- selected -->
    <item android:drawable="@drawable/ic_switch_trans" android:state_selected="true"/>
    <!-- default -->
    <item android:drawable="@drawable/ic_switch_trans"/>
</selector>