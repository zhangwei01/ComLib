<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- disabled -->
    <item android:drawable="@drawable/ic_back_disabled" android:state_enabled="false"/>
    <!-- pressed -->
    <item android:drawable="@drawable/ic_back_pressed" android:state_pressed="true"/>
    <!-- focused -->
    <item android:drawable="@drawable/ic_back_focused" android:state_focused="true"/>
    <!-- default -->
    <item android:drawable="@drawable/ic_back_normal"/>
</selector>